yara:CropGroupShape
    a              sh:NodeShape ;
    sh:targetClass yara:CropGroup ;
    sh:property    [ sh:path     yara:ODX_CropGroup_UUId ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:pattern  "^(?!\\s*$).+" ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ sh:path     yara:ODX_CropGroup_Uri ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:pattern  "^(?!\\s*$).+" ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ sh:path     yara:CropGroupName ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:pattern  "^(?!\\s*$).+" ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ sh:path     yara:CropGroupId ;
                     sh:minCount 1 ;
                     sh:maxCount 1 ;
                     sh:pattern  "^(?!\\s*$).+" ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ sh:path     yara:CG_FAOId ;
                     sh:minCount 2 ;
                     sh:maxCount 2 ;
                     sh:pattern  "^(?!\\s*$).+" ;
                     sh:datatype xsd:string ; ] ;
    sh:property    [ sh:path  yara:hasCropClass ;
                     sh:class yara:CropClass ; ] ;
.

